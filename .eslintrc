{
  "parser":"babel-eslint",
  "env":{
    "browser":true,
    "amd": true
  },
  "extends":[
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins":[
    "react"
  ],
  "settings":{
    "react":{
      "version":"detect"
    }
  },
  "rules":{
    "no-unused-vars":2,
    "sort-imports":[
      2,
      {
        "ignoreCase":true,
        "memberSyntaxSortOrder":[
          "none",
          "all",
          "single",
          "multiple"
        ]
      }
    ],
    "react/no-unused-prop-types":2,
    "react/no-unused-state":2,
    "react/jsx-closing-bracket-location":2,
    "react/jsx-closing-tag-location":2,
    "react/jsx-key":2,
    "react/jsx-wrap-multilines":2
  }
}